{
  "version": 2,
  "public": true,
  "cleanUrls": true,
  "github": {
    "enabled": false,
    "autoAlias": false,
    "silent": false
  },
  "name": "next-now",
  "alias": ["rjoydip-next-now"],
  "env": {},
  "build": {
    "env": {}
  },
  "functions": {},
  "builds": [
    { "src": "/public/**", "use": "@now/static" },
    { "src": "api/*.js", "use": "@now/node" }
  ],
  "routes": [
    { "src": "/(.+js|.+css|.+png|.+ico|robots.txt)", "dest": "/public/$1" },
    { "src": "^/precache-manifest.(.*)", "dest": "/precache-manifest.$1" }
  ]
}
